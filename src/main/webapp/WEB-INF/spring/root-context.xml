<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:beans="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans     
    http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-3.2.xsd">
    
    <context:annotation-config />
 
    <context:component-scan base-package="market.service.impl" />
    <context:component-scan base-package="market.security" />
    <context:component-scan base-package="market.interceptors" />
    <context:component-scan base-package="market.dto.assembler" />
    
    <import resource="data.xml" />
    
    <import resource="security.xml" />
    
</beans>
